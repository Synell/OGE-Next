QWidget[color='{neutral-color-name}'] QWidget[QSidePanel=true][color='main'] QPushButton:hover, QWidget[QSidePanel=true][color='{neutral-color-name}'] QPushButton:hover {
	background-color: #50{neutral-color-hover};
}

QWidget[color='{neutral-color-name}'] QWidget[QSidePanel=true][color='main'] QPushButton:focus, QWidget[QSidePanel=true][color='{neutral-color-name}'] QPushButton:focus {
