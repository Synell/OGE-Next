}

QWidget[color='{neutral-color-name}'] QWidget[QSidePanel=true][color='main'] QPushButton[selected=true], QWidget[QSidePanel=true][color='{neutral-color-name}'] QPushButton[selected=true] {
	background-color: #{neutral-color-normal};
	color: #000000;
}

QWidget[color='{neutral-color-name}'] QWidget[QSidePanel=true][color='main'] QPushButton[selected=true]:hover, QWidget[QSidePanel=true][color='{neutral-color-name}'] QPushButton[selected=true]:hover {
	background-color: #{neutral-color-normal};
	color: #000000;
