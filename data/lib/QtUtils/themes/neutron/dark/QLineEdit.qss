
/* QLineEdit */

QLineEdit {
	background-color: transparent;
	color: #FFFFFF;
	border-radius: 5px;
	padding: 0px 4px;
	border: 1px solid #313131;
	height: 32px;
}

/*QLineEdit[text=""] {
	color: #757575;
}*/

QLineEdit:hover {
	border: 1px solid #3C3C3C;
	background-color: #262626;
}

QLineEdit:focus {
	border: 1px solid #515151;
	background-color: #262626;
}
