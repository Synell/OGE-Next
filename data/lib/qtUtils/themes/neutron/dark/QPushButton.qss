QPushButton[color='{main-color-name}'], QWidget[color='{main-color-name}'] QPushButton[color='main'], QWidget[QPushButton=true][color='{main-color-name}'], QWidget[color='{main-color-name}'] QWidget[QPushButton=true][color='main'] {
