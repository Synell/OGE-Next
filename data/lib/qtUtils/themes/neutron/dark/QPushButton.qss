	background-color: #262626;
	border: 2px solid #3D77C2;
}

QPushButton[checkable=true]:hover:checked, QWidget QPushButton[checkable=true][color='main']:hover:checked, QWidget[QPushButton=true][checkable=true]:hover:checked, QWidget QWidget[QPushButton=true][checkable=true][color='main']:hover:checked {
	background-color: #333333;
}



