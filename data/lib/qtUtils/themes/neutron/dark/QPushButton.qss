/* QPushButton - Read More */

QPushButton[readmore=true] {
	height: 20px;
	border: none;
	background-color: transparent;
	font-size: 14px;
	/*padding-left: 16px;*/
	text-align: left;
	color: #999999;
	border-bottom: 2px solid #313131;
	border-radius: 0px;
}

QPushButton[readmore=true]:hover {
	color: #B3B3B3;
	border-bottom: 2px solid #484848;
}

QPushButton[readmore=true]:focus {
	color: #808080;
	border-bottom: 2px solid #313131;
}



/* QPushButton - Small */

QPushButton[small=true] {
	color: #dddddd;
	font-weight: bold;
	font-size: 12px;
}
