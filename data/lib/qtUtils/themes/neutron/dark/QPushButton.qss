
/* QPushButton */

QPushButton, QWidget[QPushButton=true] {
	background-color: #404040;
	color: #FFFFFF;
	border: 0px solid #000000;
	border-radius: 16px;
	padding: 4px 16px;
	height: 24px;
	font-weight: bold;
	font-size: 14px;
}

QPushButton:hover, QWidget[QPushButton=true]:hover {
	background-color: #343434;
}

QPushButton:pressed, QWidget[QPushButton=true]:pressed {
	background-color: #4C4C4C;
}

QPushButton:disabled, QWidget[QPushButton=true]:disabled {
	background-color: #272727;
	color: #8F8F8F;
}



/* QPushButton - Icon Padding */

QPushButton[icon-padding=true] {
	height: 24px;
	text-align: left;
	font-size: 14px;
	qproperty-iconSize: 24px;
	padding-left: 8px;
}



/* QPushButton - Transparent */

QPushButton[transparent=true], QWidget[QPushButton=true][transparent=true] {
	background-color: transparent;
	padding: 2px 14px;
	border-radius: 16px;
}

QPushButton[transparent=true]:hover, QWidget[QPushButton=true][transparent=true]:hover {
	background-color: transparent;
}

QPushButton[transparent=true]:pressed, QWidget[QPushButton=true][transparent=true]:pressed {
	background-color: transparent;
}

QPushButton[transparent=true]:disabled, QWidget[QPushButton=true][transparent=true]:disabled {
	background-color: transparent;
	color: #8F8F8F;
}



