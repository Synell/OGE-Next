/* QPushButton - Checkable */

QPushButton[checkable=true], QWidget QPushButton[checkable=true][color='main'], QWidget[QPushButton=true][checkable=true], QWidget QWidget[QPushButton=true][checkable=true][color='main'] {
	background-color: #262626;
	color: #FFFFFF;
	border: 2px solid transparent;
	border-radius: 5px;
	padding: 4px 32px;
	height: 64px;
	font-weight: bold;
	font-size: 14px;
	text-align: left;
}

QPushButton[checkable=true]:hover, QWidget QPushButton[checkable=true][color='main']:hover, QWidget[QPushButton=true][checkable=true]:hover, QWidget QWidget[QPushButton=true][checkable=true][color='main']:hover {
	background-color: #333333;
}

